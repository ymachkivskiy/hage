<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<launchConfiguration type="org.eclipse.jdt.launching.localJavaApplication">
    <booleanAttribute key="org.eclipse.debug.core.appendEnvironmentVariables" value="true"/>
    <stringAttribute key="org.eclipse.jdt.launching.CLASSPATH_PROVIDER"
                     value="org.eclipse.m2e.launchconfig.classpathProvider"/>
    <stringAttribute key="org.eclipse.jdt.launching.MAIN_TYPE" value="org.jage.platform.cli.CliNodeBootstrapper"/>
    <stringAttribute key="org.eclipse.jdt.launching.PROGRAM_ARGUMENTS"
                     value="-Dage.node.conf=file:graphite/node.xml -Dage.computation.conf=file:graphite/age.xml"/>
    <stringAttribute key="org.eclipse.jdt.launching.PROJECT_ATTR" value="org.jage.student.examples-applications"/>
    <stringAttribute key="org.eclipse.jdt.launching.SOURCE_PATH_PROVIDER"
                     value="org.eclipse.m2e.launchconfig.sourcepathProvider"/>
    <stringAttribute key="org.eclipse.jdt.launching.WORKING_DIRECTORY"
                     value="${project_loc:org.jage.student.examples-applications}/target/classes/examples/monitoring/"/>
</launchConfiguration>
