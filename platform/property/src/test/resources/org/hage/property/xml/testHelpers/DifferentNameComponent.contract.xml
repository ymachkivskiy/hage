<?xml version="1.0" encoding="UTF-8"?>
<component
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        class="org.hage.property.xml.testHelpers.AdvancedExampleComponent" version="1.0.0"
        xmlns="org.jage" xsi:schemaLocation="org.jage ageDependency.xsd">
    <extends class="org.hage.property.xml.testHelpers.SimpleExampleComponent"/>
    <declaration>
        <attribute access="setter" modifier="2" name="intProperty" required="true" getter="getIntProperty"
                   setter="setIntProperty">
            <type>
                <primitive name="int"/>
            </type>
        </attribute>
        <attribute access="setter" modifier="2" name="stringProperty" required="true" getter="getStringProperty"
                   setter="setStringProperty">
            <type>
                <simpleClass class="java.lang.String" interface="false"/>
            </type>
        </attribute>
    </declaration>
</component>
