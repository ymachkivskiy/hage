<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<launchConfiguration type="org.eclipse.jdt.launching.localJavaApplication">
    <booleanAttribute key="org.eclipse.debug.core.appendEnvironmentVariables" value="true"/>
    <stringAttribute key="org.eclipse.jdt.launching.MAIN_TYPE" value="CliNodeBootstrapper"/>
    <stringAttribute key="org.eclipse.jdt.launching.PROGRAM_ARGUMENTS"
                     value="-Dage.node.conf=classpath:node.xml -Dage.computation.conf=classpath:age.xml"/>
    <stringAttribute key="org.eclipse.jdt.launching.PROJECT_ATTR" value="org.jage.algorithms-vanilla-genetic"/>
    <stringAttribute key="org.eclipse.jdt.launching.VM_ARGUMENTS"
                     value="-Dage.config.properties=classpath:age.properties"/>
    <stringAttribute key="org.eclipse.jdt.launching.WORKING_DIRECTORY"
                     value="${project_loc:org.jage.algorithms-vanilla-genetic}/target/classes/"/>
</launchConfiguration>
